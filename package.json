{
    "name": "line-link",
    "displayName": "LineLink",
    "description": "Open a link to your exact line of code in a browser from VS Code. Compatible with GitHub and GitLab.",
    "version": "0.0.1",
    "author": {
        "name": "Kevin Rose",
        "url": "https://github.com/kbrose"
    },
    "keywords": [
        "link",
        "github",
        "gitlab",
        "web",
        "browser"
    ],
    "qna": false,
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": false,
            "description": "A hostile repository could open a malicious link."
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kbrose/vsc-line-link"
    },
    "readme": "README.md",
    "license": "MIT",
    "extensionKind": [
        "workspace",
        "ui"
    ],
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:line-link.copyLink",
        "onCommand:line-link.openLink"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "line-link.copyLink",
                "title": "Copy Link To Code"
            },
            {
                "command": "line-link.openLink",
                "title": "Open in Browser"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.74.0",
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "glob": "^8.0.3",
        "mocha": "^10.1.0",
        "typescript": "^4.9.3",
        "@vscode/test-electron": "^2.2.0"
    },
    "extensionDependencies": [
        "vscode.git"
    ]
}
